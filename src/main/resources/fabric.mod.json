{
  "schemaVersion": 1,
  "id": "moonmc",
  "version": "${version}",
  "name": "MoonMC",
  "description": "Minecraft but I'm on the moon",
  "authors": [
    "isXander"
  ],
  "contact": {
    "homepage": "https://www.isxander.dev",
    "issues": "https://github.com/isXander/MoonMC/issues",
    "sources": "https://github.com/isXander/MoonMC"
  },
  "license": "ARR",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.isxander.moonmc.MoonMod"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.isxander.moonmc.client.MoonClientMod"
      }
    ],
    "kambrik": [
      {
        "adapter": "kotlin",
        "value": "dev.isxander.moonmc.registry.MoonRegistry"
      }
    ]
  },
  "mixins": [
    "moonmc.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": ">=0.12.0",
    "fabric-language-kotlin": ">=1.7.0+kotlin.1.6.0",
    "minecraft": "1.18.x",
    "java": ">=17",
    "kambrik": ">=3.0.0"
  }
}
